<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:dashGap="0dp"
        android:dashWidth="6dp"
        android:width="3dp"
        android:color="@color/blue" />
    <!-- 矩形的圆角半径 -->
    <corners android:radius="10dp" />
    <solid
        android:color="#fff"/>
</shape>